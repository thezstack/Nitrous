<p id="notice"><%= notice %></p>

<h1>Listing Ulistings</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Postition</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ulistings.each do |ulisting| %>
      <tr>
        <td><%= ulisting.first_name %></td>
        <td><%= ulisting.last_name %></td>
        <td><%= ulisting.email %></td>
        <td><%= ulisting.postition %></td>
        <td><%= link_to 'Show', ulisting %></td>
        <td><%= link_to 'Edit', edit_ulisting_path(ulisting) %></td>
        <td><%= link_to 'Destroy', ulisting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ulisting', new_ulisting_path %>
